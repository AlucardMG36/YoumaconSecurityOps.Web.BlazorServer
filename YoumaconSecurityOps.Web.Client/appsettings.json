    "Domain": "ysecops.app",
    "TenantId": "2e94b692-3a98-4d83-8e1b-6ccfc63234f5",
    "ClientId": "0dfd78f3-f81c-4e3e-9eb8-df2dd6f2c2bf",
    "ClientSecret": "3ks7Q~ZaBcSSlPbqGQb28ztwGx~Jf_yKy3DuW",
